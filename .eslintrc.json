{
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "plugins": [
        "import"
    ],
    "rules": {
      "no-console": "off",
      "semi": [
          "error",
          "always"
      ]
    },
    "globals": {
      "$": 1
    }
}
